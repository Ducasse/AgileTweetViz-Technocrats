Object subclass: #CSVReader	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'CSVParser'!!CSVReader methodsFor: 'as yet unclassified' stamp: 'AdityaBivalkar 10/30/2015 19:30'!parseCSV|stream  line parts location locationDict timezone latitude longitude   i|stream := FileStream readOnlyFileNamed: 'LocationsNew.csv'.locationDict:=Dictionary new.i:=1.line := stream nextLine.[stream atEnd] whileFalse:[line := stream nextLine.parts:=(line subStrings:(',')).timezone:=parts at:1.latitude:=parts at:2.longitude:=parts at:3.location:=Location new.location timezone:timezone.location latitude:latitude.location longitude:longitude.locationDict at:timezone put: location.i:=i+1. 	]. ^locationDict.! !